  "type": "library",
  "keywords": ["yii2", "extension", "flash messages", "framework", "php"],
  "license": "BSD-3-Clause",
  "version": "3.0.0",
  "authors": [
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4.0",
    "nox-it/yii2-nox": "~2.0.0",
